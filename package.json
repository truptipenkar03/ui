{
  "name": "@rtkwlf/ui",
  "version": "0.0.8",
  "main": "lib/index.js",
  "license": "MIT",
  "typings": "lib/index.d.ts",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "svg": "node_modules/@svgr/cli/bin/svgr --ext tsx -d src/components/icons assets/icons/",
    "build": "rm -rf lib/ && npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:ts": "tsc",
    "build:js": "BABEL_ENV=build babel ./src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "storybook": "start-storybook -p 9000 --quiet -s public",
    "storybook:build": "build-storybook -o build/storybook/static -s public",
    "storybook:docs": "build-storybook --docs -o docs -s public",
    "storybook:deploy": "storybook-to-ghpages -- --existing-output-dir=/docs",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "coverage:open": "open ./coverage/lcov-report/index.html",
    "lint": "eslint '**/*.{ts,tsx}'",
    "lint:fix": "eslint '**/*.{ts,tsx}' --fix"
  },
  "dependencies": {
    "@types/styled-components": "4.4.0",
    "framer-motion": "^1.6.12",
    "styled-components": "5.0.0-rc.2"
  },
  "peerDependencies": {
    "react": "16.11.0",
    "react-dom": "16.11.0",
    "react-is": "16.11.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.0",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "7.2.1",
    "@babel/plugin-proposal-object-rest-spread": "7.2.0",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@storybook/addon-docs": "5.3.0-alpha.45",
    "@storybook/addon-storysource": "5.3.0-alpha.45",
    "@storybook/addons": "5.3.0-alpha.45",
    "@storybook/client-api": "5.3.0-alpha.45",
    "@storybook/preset-typescript": "1.1.0",
    "@storybook/react": "5.3.0-alpha.45",
    "@storybook/source-loader": "5.3.0-alpha.45",
    "@svgr/cli": "^4.3.2",
    "@testing-library/react-hooks": "3.2.1",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.18",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.1",
    "@types/react-test-renderer": "^16.9.0",
    "@types/storybook__addon-info": "^4.1.2",
    "@types/storybook__react": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "2.6.1",
    "@typescript-eslint/parser": "2.6.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-styled-components": "^1.10.0",
    "cache-loader": "^3.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "6.6.0",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.2.0",
    "install": "^0.13.0",
    "jest": "^24.8.0",
    "jest-styled-components": "^7.0.0-beta.1",
    "npm": "^6.12.0",
    "prop-types": "^15.7.2",
    "react-docgen-typescript": "^1.15.0",
    "react-docgen-typescript-loader": "3.4.0",
    "react-dom": "16.12.0",
    "react-test-renderer": "^16.9.0",
    "ts-loader": "6.2.1",
    "typescript": "^3.6.4"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./setupTest.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.stories.tsx",
      "!src/**/*.components.tsx",
      "!src/**/icons/**",
      "!src/styled/**",
      "!src/theme/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 86,
        "functions": 80,
        "lines": 87,
        "statements": 87
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/rtkwlf/ui.git"
  }
}
